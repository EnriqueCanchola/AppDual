
@{
    ViewData["Title"] = "Registro";
}

<h2>Registro Jefe De Carrera</h2>

<h5>Información Personal</h5>
<div class="container">
    <div class="row">
        <form>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Nombre:</label>
                    <input type="text" class="form-control" id="" placeholder="Nombre">
                </div>

            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Apellidos:</label>
                    <input type="text" class="form-control" id="" placeholder="Nombre">
                </div>

            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="form-group">
                    <label>Teléfono:</label>
                    <input type="text" class="form-control" id="" placeholder="Teléfono">
                </div>

            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Email:</label>
                    <input type="text" class="form-control" id="" placeholder="Nombre">
                </div>

            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Divición:</label>
                    <select id="" class="form-control">
                        <option selected>Selecciona una Divición...</option>
                        <option>ISC</option>
                    </select>
                </div>
            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Usuario:</label>
                    <input type="text" class="form-control" id="" placeholder="Nombre">
                </div>

            </div>


            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Tecnologico:</label>
                    <select id="" class="form-control">
                        <option selected>Selecciona un tecnologico...</option>
                        <option>Tecnologico de Ecatepec</option>
                    </select>
                </div>
            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-group">
                    <label>Contraseña:</label>
                    <input type="text" class="form-control" id="" placeholder="Nombre">
                </div>

            </div>

            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                <div class="form-check form-check-inline">

                    <label class="form-check-label">Genero: </label>

                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="Hombre">
                    <label class="form-check-label">Hombre</label>

                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="Mujer">
                    <label class="form-check-label">Mujer</label>

                </div>

            </div>


            <div class="col-lg-6 col-sm-6 col-md-6 col-xs-12">
                <div class="form-group">
                    <button class="btn btn-primary" type="submit">Guardar</button>
                    <button class="btn btn-danger" type="reset">Canselar</button>
                </div>
            </div>

        </form>
    </div>
</div>